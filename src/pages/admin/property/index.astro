---
import AdminPropertiesList from "@components/AdminPropertiesList";
import Dashboard from "@layouts/Dashboard.astro";

const userRoleId = Astro.locals.currentUserRoleId;
const userId = Astro.locals.currentUserId;
---

<Dashboard pageTitle="Propiedades">
  <h1 class="mb-6 font-semibold text-lg block">Propiedades</h1>
  <AdminPropertiesList client:load userRoleId={userRoleId} userId={userId} />
</Dashboard>
